<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DairyProducts_Activity"
    tools:layout_editor_absoluteY="25dp">

    <TextView
        android:id="@+id/milk"
        android:layout_width="94dp"
        android:layout_height="34dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="72dp"
        android:gravity="start"
        android:text="@string/milk"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/milkimg"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/egg"
        android:layout_width="94dp"
        android:layout_height="34dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="16dp"
        android:text="@string/egg"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/egggimg"
        app:layout_constraintTop_toBottomOf="@+id/yogurtPrice" />

    <TextView
        android:id="@+id/yogurt"
        android:layout_width="94dp"
        android:layout_height="34dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="24dp"
        android:text="@string/yogurt"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/yogutimg"
        app:layout_constraintTop_toBottomOf="@+id/wippingprice" />

    <TextView
        android:id="@+id/wipping"
        android:layout_width="89dp"
        android:layout_height="48dp"
        android:layout_marginStart="22dp"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="16dp"
        android:text="@string/wipping"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/whippingimg"
        app:layout_constraintTop_toBottomOf="@+id/cheesePrice" />

    <EditText
        android:id="@+id/yougurtQty"
        android:layout_width="69dp"
        android:layout_height="41dp"
        android:layout_marginStart="64dp"
        android:layout_marginLeft="64dp"
        android:layout_marginTop="56dp"
        android:ems="10"
        android:hint="@string/qty"
        android:inputType="number"
        app:layout_constraintStart_toEndOf="@+id/yogurt"
        app:layout_constraintTop_toBottomOf="@+id/wippingQty"
        tools:ignore="Autofill" />

    <EditText
        android:id="@+id/eggsQty"
        android:layout_width="69dp"
        android:layout_height="41dp"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="56dp"
        android:ems="10"
        android:hint="@string/qty"
        android:inputType="number"
        app:layout_constraintStart_toEndOf="@+id/egg"
        app:layout_constraintTop_toBottomOf="@+id/yougurtQty"
        tools:ignore="Autofill" />

    <TextView
        android:id="@+id/eggPrice"
        android:layout_width="96dp"
        android:layout_height="29dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="@string/eggPrice"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/egggimg"
        app:layout_constraintTop_toBottomOf="@+id/egg" />

    <TextView
        android:id="@+id/yogurtPrice"
        android:layout_width="96dp"
        android:layout_height="29dp"
        android:layout_marginStart="17dp"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="9dp"
        android:text="@string/yogurtPrice"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/yogutimg"
        app:layout_constraintTop_toBottomOf="@+id/yogurt" />

    <TextView
        android:id="@+id/cheese"
        android:layout_width="94dp"
        android:layout_height="34dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="12dp"
        android:text="@string/cheese"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/cheeseimg"
        app:layout_constraintTop_toBottomOf="@+id/milkPrice" />

    <TextView
        android:id="@+id/wippingprice"
        android:layout_width="96dp"
        android:layout_height="29dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="8dp"
        android:text="@string/wippingPrice"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/whippingimg"
        app:layout_constraintTop_toBottomOf="@+id/wipping" />

    <TextView
        android:id="@+id/milkPrice"
        android:layout_width="96dp"
        android:layout_height="29dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="4dp"
        android:text="@string/milkPice"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/milkimg"
        app:layout_constraintTop_toBottomOf="@+id/milk" />

    <EditText
        android:id="@+id/wippingQty"
        android:layout_width="69dp"
        android:layout_height="41dp"
        android:layout_marginStart="64dp"
        android:layout_marginLeft="64dp"
        android:layout_marginTop="44dp"
        android:ems="10"
        android:hint="@string/qty"
        android:inputType="number"
        app:layout_constraintStart_toEndOf="@+id/wipping"
        app:layout_constraintTop_toBottomOf="@+id/cheeseQty"
        tools:ignore="Autofill" />

    <TextView
        android:id="@+id/cheesePrice"
        android:layout_width="96dp"
        android:layout_height="30dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="8dp"
        android:text="@string/cheesePrice"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/cheeseimg"
        app:layout_constraintTop_toBottomOf="@+id/cheese" />

    <EditText
        android:id="@+id/milkQty"
        android:layout_width="69dp"
        android:layout_height="41dp"
        android:layout_marginStart="64dp"
        android:layout_marginLeft="64dp"
        android:layout_marginTop="72dp"
        android:ems="10"
        android:hint="@string/qty"
        android:inputType="number"
        app:layout_constraintStart_toEndOf="@+id/milk"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="Autofill" />

    <EditText
        android:id="@+id/cheeseQty"
        android:layout_width="69dp"
        android:layout_height="41dp"
        android:layout_marginStart="64dp"
        android:layout_marginLeft="64dp"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:hint="@string/qty"
        android:inputType="number"
        app:layout_constraintStart_toEndOf="@+id/cheese"
        app:layout_constraintTop_toBottomOf="@+id/milkQty"
        tools:ignore="Autofill" />

    <Button
        android:id="@+id/dairyAdd"
        android:layout_width="142dp"
        android:layout_height="66dp"
        android:layout_marginStart="236dp"
        android:layout_marginLeft="236dp"
        android:layout_marginTop="60dp"
        android:text="@string/addToCart"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eggsQty" />

    <ImageView
        android:id="@+id/milkimg"
        android:layout_width="77dp"
        android:layout_height="66dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="72dp"
        android:contentDescription="@string/milk"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/milk" />

    <ImageView
        android:id="@+id/cheeseimg"
        android:layout_width="80dp"
        android:layout_height="69dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="19dp"
        android:contentDescription="@string/cheese"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/milkimg"
        app:srcCompat="@drawable/cheese" />

    <ImageView
        android:id="@+id/yogutimg"
        android:layout_width="71dp"
        android:layout_height="75dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="14dp"
        android:contentDescription="@string/yogurt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/whippingimg"
        app:srcCompat="@drawable/yogurt" />

    <ImageView
        android:id="@+id/whippingimg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="17dp"
        android:contentDescription="@string/wipping"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cheeseimg"
        app:srcCompat="@drawable/wip" />

    <ImageView
        android:id="@+id/egggimg"
        android:layout_width="71dp"
        android:layout_height="80dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="4dp"
        android:contentDescription="@string/egg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yogutimg"
        app:srcCompat="@drawable/eg" />

</android.support.constraint.ConstraintLayout>